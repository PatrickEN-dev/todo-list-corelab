generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Card {
  id         String   @id
  title      String   @unique
  note       String
  isFavorite Boolean  @default(false)
  color      String?
  createdAt  DateTime @default(now())
}
